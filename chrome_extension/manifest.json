{
  "name": "AmazonCaffeineComparator",
  "action": {},
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "Amazon商品ページからレッドブルのカフェイン摂取効率を比較するChrome拡張です",
  "icons":{
    "16":"energy.png",
    "48":"energy.png",
    "128":"energy.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [{
    "run_at": "document_end",
    "matches": ["http://www.amazon.co.jp/*", "https://www.amazon.co.jp/*"],
    "js": ["jquery-3.6.0.min.js", "script.js"]
  }]
}